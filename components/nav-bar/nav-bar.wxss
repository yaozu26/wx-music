.nav-bar {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  display: flex;
  height: 44px;
  color: #fff;
}

.left,.center,.right {
  display: flex;
  justify-content: center;
  align-items: center;
}

.left, .right {
  width: 120rpx;
}

.center {
  flex: 1;
}


.default {
  display: none;
}

.slot:empty + .default {
  display: flex;
}